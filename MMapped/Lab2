#Read two digits with memory-mapped I/O
#Convert the two digits (ASCII Characters) into a decimal value
#Print the decimal value through the syscall


        .data
num:	.word	2
        .text
        .globl main

main:


rd_wait:
		lw		$t1, 0($t0)
		andi 	$t1, $t1, 0x0001 	
		beq		$t1, $zero, rd_wait     
		lw		$s0, 4($t0)		
